<div class="request-container">
  <h1 class="request-title">Make New Request</h1>
  <div class="request-form">
  <%= simple_form_for [@dog, @request] do |f|%>
      <%= f.input :content, label: "Why?" %>
      <%= f.input :start_date, html5: true %>
      <%= f.input :end_date, html5:true %>
      <%= f.submit "Make Request", class: "btn btn-outline-dark" %>
  <% end %>
</div>
</div>
